<div class="controls-div">
<table class="diff sourcecode file-source" cellspacing="0" cellpadding="0">
	<thead>
		<tr class="controls">
			<td>
				<div class="control-head">
				<p class="controlblock">
					<%= link_to 'back', '', :class => 'back' %>
					<%= link_to 'forward', '', :class => 'forward' %>
					<%#= link_to_node 'back', @node, previous_changeset.revision, :class => 'back' if previous_changeset %>
					<%#= link_to_node 'forward', @node, next_changeset.revision, :class => 'forward' if @revision && next_changeset %>
				</p>
				<ul class="formats">
					<li><%= link_to 'Blame', link_to_blame(@node) %><%#= link_to_blame 'Blame', @node %></li>
					<li><%= link_to 'History', '' %><%#= link_to_history 'History', @node %></li>
					<li<% unless @node.text? %> class="end"<% end %>><%= link_to 'Raw', link_to_raw_for_node(@node) %></li>
					<% if @node.text? -%><li class="end"><%= link_to 'Text', link_to_text_for_node(@node) %></li><% end -%>
				</ul>
				</div>
			</td>
		</tr>
		<tr>
			<th><%= @node.path.split('/').last %></th>
		</tr>
	</thead>
</table>
</div>
<% if controller.action_name != 'blame' -%>
<div class="source-div">
	<div id="source"><pre class="viewsource" style="width:90%;"><code>
	<%# cache do %><%#= highlight_source_for_node(@node) %><%# end %></code></pre></div>
</div>
<% else %>
<div class="blame-div">
	<table wdith="100%" class="blame" cellpadding="0" cellspacing="0">
		<!-- <tr>
			<th>&nbsp;</th>
			<th>&nbsp;</th>
			<th>&nbsp;</th>
